Cell[TextData[{
 "\n",
 StyleBox["Advertising:",
  FontColor->RGBColor[0, 0, 1]],
 "\nHave you ever decide to calculate the Einstein tensor ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["G", "\[Mu]\[Nu]"], TraditionalForm]]],
 " but, after 3 hours, failed?!? If \[OpenCurlyDoubleQuote]yes\
\[CloseCurlyDoubleQuote], just like me, this programme may help you, lazy man!"
}], "Text",
 CellChangeTimes->{{3.549851153279598*^9, 3.54985115830301*^9}, {
  3.549851238990177*^9, 3.5498512917202053`*^9}, {3.54985245407978*^9, 
  3.549852540464355*^9}}],

Cell[TextData[{
 "\n",
 StyleBox["Illustration:",
  FontColor->RGBColor[0, 0, 1]],
 "\nA way to calculate the tensors in Einstein\[CloseCurlyQuote]s field \
equations. You can change the information of the ",
 StyleBox["dim",
  FontSlant->"Italic"],
 " and the ",
 StyleBox["metric",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "\[Mu]\[Nu]"], TraditionalForm]]],
 ", then the programme here can help calculate the components of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    SuperscriptBox["\[CapitalGamma]", "\[Alpha]"], "\[Beta]\[Gamma]"], 
   TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", "\[Alpha]\[Beta]\[Gamma]\[Delta]"], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", "\[Alpha]\[Beta]"], TraditionalForm]]],
 ", R, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["G", "\[Alpha]\[Beta]"], TraditionalForm]]],
 ", and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["G", "\[Alpha]\[Beta]"], TraditionalForm]]],
 ", and other quantities that depend on these in ANY n-dim space-time."
}], "Text",
 CellChangeTimes->{{3.5498516233851*^9, 3.5498519658825073`*^9}, {
   3.549852356974639*^9, 3.549852358809619*^9}, {3.549852587876088*^9, 
